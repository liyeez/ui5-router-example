<mvc.View 
    controllerName="sap.btp.sapui5.controller.List"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page title="{i18n>ListTitle}">
        <List id="list" items="{/Products}">
            <StandardListItem 
                type="Navigation" 
                press="handleListItemPress"
                title="{ProductName}"/>
        </List>
    </Page>   
    
</mvc.View>